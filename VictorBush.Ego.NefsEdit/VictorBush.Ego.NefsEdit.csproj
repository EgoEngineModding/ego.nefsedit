    </Compile>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\VictorBush.Ego.NefsCommon\VictorBush.Ego.NefsCommon.InjectionDatabase\VictorBush.Ego.NefsCommon.InjectionDatabase.csproj" />
